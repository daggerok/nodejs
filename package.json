{
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dist": "npm i && rimraf -rf dist && mkdir -p dist",
    "mocha": "npm i && mocha --compilers es6:babel-register",
    "build": "npm i && babel -d dist index.es6 lib test",
    "browser": "npm run dist && browserify --extension=.es6 -t babelify index.es6 -o dist/bundle.js",
    "test": "npm run mocha",
    "once": "npm i && node index.es6",
    "tdd": "npm run mocha -- -w --watch-extensions es6",
    "start": "npm i && nodemon index.es6"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs",
      [
        "transform-strict-mode",
        {
          "strict": true
        }
      ]
    ]
  },
  "keywords": [
    "node",
    "babel",
    "es6",
    "mocha",
    "chai"
  ],
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.7.0",
    "babel-plugin-transform-strict-mode": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "chai": "^3.5.0",
    "mkdir": "0.0.2",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "rimraf": "^2.5.2"
  },
  "name": "node-babel-es6-starter",
  "description": "node.js maps",
  "main": "app.js",
  "devDependencies": {},
  "author": "Maksim Kostromin <daggerok@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/daggerok/nodejs/issues"
  },
  "homepage": "https://github.com/daggerok/nodejs#readme"
}
