{
  "name": "nodejs",
  "version": "0.0.1",
  "private": true,
  "main": "./src/express/server.es6",
  "scripts": {
    "sleep": "sleep 1",
    "postinstall": "npm prune",
    "start": "npm-run-all -p server open-url",
    "open-url": "npm-run-all sleep open",
    "open": "open-url http://localhost:3000",
    "test": "babel-node src/express/server.es6",
    "server": "nodemon src/express/server.es6 --exec babel-node",
    "build": "babel src -d dist",
    "run-express": "node dist/express/server.js",
    "run-hapi": "node dist/hapi/server.js",
    "run-koa": "node dist/koa/server.js",
    "express": "npm-run-all build run-express",
    "hapi": "npm-run-all build run-hapi",
    "koa": "npm-run-all build run-koa",
    "serve": "npm-run-all -p open-url express"
  },
  "devDependencies": {
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "express": "^4.13.4",
    "hapi": "^13.0.0",
    "koa": "^1.2.0",
    "koa-route": "^2.4.2",
    "node-babel": "^0.1.2",
    "nodemon": "^1.9.1",
    "npm-run-all": "^3.1.1"
  },
  "keywords": [
    "node",
    "babel",
    "node-babel",
    "es6",
    "express",
    "hapi",
    "koa"
  ],
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "hbs": "~4.0.0",
    "morgan": "~1.7.0",
    "npm-run-all": "^3.1.1",
    "open-url": "^2.0.2",
    "serve-favicon": "~2.3.0",
    "stylus": "0.54.5"
  },
  "description": "node.js maps",
  "author": "Maksim Kostromin <daggerok@gmail.com>",
