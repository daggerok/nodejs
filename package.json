{
  "name": "nodejs",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=5.7.1",
    "npm": ">=3.6.0"
  },
  "config": {
    "bower_directory": "static/vendor"
  },
  "scripts": {
    "sleep": "sleep 1",
    "postinstall": "bower i",
    "test": "babel-node server.es6",
    "open-url": "npm-run-all -p sleep open",
    "open": "open-url http://localhost:3000",
    "start": "nodemon server.es6 --exec babel-node",
    "build": "babel *.es6 **/*.es6 -d .",
    "server": "node server.js",
    "server-and-open-url": "npm-run-all -p open-url server",
    "serve": "npm-run-all build server-and-open-url"
  },
  "devDependencies": {
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "bower": "^1.7.7",
    "express": "^4.13.4",
    "hbs": "^4.0.0",
    "morgan": "^1.7.0",
    "node-babel": "^0.1.2",
    "nodemon": "^1.9.1",
    "npm-run-all": "^3.1.1",
    "open-url": "^2.0.2"
  },
  "description": "Node and Express using Babel ES6",
  "main": "server.es6",
  "author": "Maksim Kostromin <daggerok@gmail.com>",
  "keywords": [
    "node",
    "babel",
    "node-babel",
    "ES6",
    "express",
    "morgan",
    "handlebars",
    "hbs"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/nodejs.git"
  },
  "bugs": {
    "url": "https://github.com/daggerok/nodejs/issues"
  },
  "homepage": "https://github.com/daggerok/nodejs#readme"
}
